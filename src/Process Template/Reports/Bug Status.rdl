          </QueryParameter>
        </QueryParameters>
        <CommandText>;WITH IterationKey(IterationSK, IterationName, Depth, IterationKey) AS
(
	SELECT IterationSK, IterationName, Depth,
		'.&amp;[' +
			CONVERT(nvarchar, CONVERT(bigint, SUBSTRING(CONVERT(binary(16), CONVERT(uniqueidentifier, IterationGUID)), 1, 8))) +
			']&amp;[' +
			CONVERT(nvarchar, CONVERT(bigint, SUBSTRING(CONVERT(binary(16), CONVERT(uniqueidentifier, IterationGUID)), 9, 8))) +
		']'
	  FROM DimIteration
)

SELECT '*' AS IterationSK, 'All' AS IterationName
WHERE '[Work Item].[Iteration Hierarchy].[All]' IN (@IterationParam)

UNION

SELECT DISTINCT 
	CASE WHEN Depth = 0 THEN '*' ELSE CAST(IterationSK AS NVARCHAR(255)) END AS IterationSK, 
	CASE WHEN Depth = 0 THEN 'All' ELSE IterationName END
  FROM IterationKey
 WHERE
	'[Work Item].[Iteration Hierarchy].[Iteration' + CONVERT(varchar, Depth) + ']' + IterationKey IN (@IterationParam)
    OR	
	'[Work Item].[Iteration Hierarchy].[Iteration' + CONVERT(varchar, Depth + 1) + ']' + IterationKey IN (@IterationParam)</CommandText>
